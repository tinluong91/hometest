  "license": "MIT",
  "private": true,
  "engines": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@graphql-codegen/add": "4.0.0",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/plugin-helpers": "4.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@swc-node/register": "1.6.3",
    "@swc/core": "1.3.44",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.13",
    "@types/jest": "29.5.2",
    "@types/node": "18.13.0",
    "@types/ramda": "0.28.22",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "dotenv": "16.0.3",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "46.2.6",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "47.0.0",
    "faker": "6.6.6",
    "graphql-tag": "2.12.6",
    "jest": "29.5.0",
    "node-fetch": "3.3.1",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@apollo/federation": "0.38.1",
    "@apollo/server": "4.7.4",
    "@prisma/client": "4.15.0",
    "body-parser": "1.20.1",
    "cors": "2.8.5",
    "dataloader": "2.2.1",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "io-ts": "2.2.20",
    "ramda": "0.28.0"
  },
  "scripts": {
    "build": "node_modules/typescript/bin/tsc",
    "lint": "npx eslint ./src/**/*.ts ./__tests__/**/*.ts",
    "lint-ci": "npx eslint -f json -o ./eslint-report.json ./src/**/*.ts ./__tests__/**/*.ts",
    "test": "jest",
    "test:cov": "jest --coverage --maxWorkers=20% ",
    "postinstall": "npm run prisma:generate",
    "dev": "ts-node-dev --watch -r tsconfig-paths/register -r dotenv/config src/server.ts",
    "prisma:generate": "prisma generate --schema ./src/shared/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ./src/shared/prisma/schema.prisma",
    "prisma:db:push": "prisma db push --schema ./src/shared/prisma/schema.prisma",
    "codegen": "graphql-codegen --config codegen.ts",
    "seed": "node -r @swc-node/register -r dotenv/config -r tsconfig-paths/register ./src/seed/index.ts"
  }
}
